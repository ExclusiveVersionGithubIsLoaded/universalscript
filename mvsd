    PremiumOnly = false
})

-- –°–ª–∞–π–¥–µ—Ä –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ –≥–æ–ª–æ–≤—ã
TabHitbox:AddSlider({
    Name = "head size",
    Min = 1,
    Max = 100,
    Default = 15,
    Color = Color3.fromRGB(127, 255, 212), -- –∞–∫–≤–∞–º–∞—Ä–∏–Ω–æ–≤—ã–π —Ü–≤–µ—Ç
    Increment = 1,
    ValueName = "Head Size",
    Callback = function(Value)
        _G.HeadSize = Value
    end    
})

-- –ß–µ–∫–±–æ–∫—Å –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è/–æ—Ç–∫–ª—é—á–µ–Ω–∏—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ –≥–æ–ª–æ–≤—ã
TabHitbox:AddToggle({
    Name = "enable head resize",
    Default = false,
    Callback = function(Value)
        _G.DisabledHeadResize = not Value
        ResizeHead()
    end    
})

-- –°–ª–∞–π–¥–µ—Ä –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ —Ç—É–ª–æ–≤–∏—â–∞
TabHitbox:AddSlider({
    Name = "torso size",
    Min = 1,
    Max = 100,
    Default = 15,
    Color = Color3.fromRGB(127, 255, 212), -- –∞–∫–≤–∞–º–∞—Ä–∏–Ω–æ–≤—ã–π —Ü–≤–µ—Ç
    Increment = 1,
    ValueName = "Torso Size",
    Callback = function(Value)
        _G.TorsoSize = Value
    end    
})

-- –ß–µ–∫–±–æ–∫—Å –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è/–æ—Ç–∫–ª—é—á–µ–Ω–∏—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ —Ç—É–ª–æ–≤–∏—â–∞
