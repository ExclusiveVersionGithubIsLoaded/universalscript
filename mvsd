-- –ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ Orion Library
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- –°–æ–∑–¥–∞–Ω–∏–µ –æ–∫–Ω–∞
local Window = OrionLib:MakeWindow({
    Name = "üåÄüåÄVersion HubüåÄüåÄ | script by version", 
    HidePremium = false, 
    SaveConfig = true, 
    ConfigFolder = "OrionConfig"
})

-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è –Ω–∞—Å—Ç—Ä–æ–µ–∫
_G.HeadSize = 1
_G.TorsoSize = 1
_G.DisabledHeadResize = true
_G.DisabledTorsoResize = true
_G.HighlightEnabled = false
_G.ShowNames = false -- –ù–∏–∫–Ω–µ–π–º—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é –≤—ã–∫–ª—é—á–µ–Ω—ã
_G.HighlightColor = Color3.fromRGB(255, 0, 0)
_G.NameTagColor = Color3.fromRGB(255, 0, 0) -- –û—Ç–¥–µ–ª—å–Ω—ã–π —Ü–≤–µ—Ç –¥–ª—è –Ω–∏–∫–Ω–µ–π–º–æ–≤

-- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è –¥–ª—è —Å–∫–æ—Ä–æ—Å—Ç–∏ –∏ –ø—Ä—ã–∂–∫–∞
local defaultWalkSpeed = game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed
local defaultJumpPower = game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower

_G.WalkSpeed = defaultWalkSpeed
_G.JumpPower = defaultJumpPower
_G.SpeedEnabled = false
_G.JumpEnabled = false

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Å–∫–æ—Ä–æ—Å—Ç–∏
local function SetWalkSpeed()
    local player = game:GetService("Players").LocalPlayer
    if _G.SpeedEnabled then
        player.Character.Humanoid.WalkSpeed = _G.WalkSpeed
    else
        player.Character.Humanoid.WalkSpeed = defaultWalkSpeed
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è –ø—Ä—ã–∂–∫–∞
local function SetJumpPower()
    local player = game:GetService("Players").LocalPlayer
    if _G.JumpEnabled then
        player.Character.Humanoid.JumpPower = _G.JumpPower
    else
        player.Character.Humanoid.JumpPower = defaultJumpPower
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–æ–≤ –≥–æ–ª–æ–≤—ã
local function ResizeHead()
    game:GetService('RunService').RenderStepped:Connect(function()
        for _, player in next, game:GetService('Players'):GetPlayers() do
            if player.Name ~= game:GetService('Players').LocalPlayer.Name and player.Character and player.Character:FindFirstChild("Head") then
                if not _G.DisabledHeadResize then
                    player.Character.Head.Size = Vector3.new(_G.HeadSize, _G.HeadSize, _G.HeadSize)
                    player.Character.Head.Transparency = 0.75 -- 75% –ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å –≥–æ–ª–æ–≤—ã
                    player.Character.Head.CanCollide = false -- –û—Ç–∫–ª—é—á–µ–Ω–∏–µ –∫–æ–ª–ª–∏–∑–∏–∏
                    player.Character.Head.Massless = true -- –û—Ç–∫–ª—é—á–µ–Ω–∏–µ –º–∞—Å—Å—ã –¥–ª—è –≥–æ–ª–æ–≤—ã
                else
                    player.Character.Head.Size = Vector3.new(2, 1, 1) -- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π —Ä–∞–∑–º–µ—Ä –≥–æ–ª–æ–≤—ã
                    player.Character.Head.Transparency = 0 -- –ü–æ–ª–Ω–æ—Å—Ç—å—é –≤–∏–¥–∏–º–∞—è
                end
            end
        end
    end)
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–æ–≤ —Ç—É–ª–æ–≤–∏—â–∞
local function ResizeTorso()
    game:GetService('RunService').RenderStepped:Connect(function()
        for _, player in next, game:GetService('Players'):GetPlayers() do
            if player.Name ~= game:GetService('Players').LocalPlayer.Name and player.Character and (player.Character:FindFirstChild("Torso") or player.Character:FindFirstChild("UpperTorso")) then
                local torso = player.Character:FindFirstChild("Torso") or player.Character:FindFirstChild("UpperTorso")
                if not _G.DisabledTorsoResize then
                    torso.Size = Vector3.new(_G.TorsoSize, _G.TorsoSize, _G.TorsoSize)
                    torso.Transparency = 0.75 -- 75% –ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å —Ç—É–ª–æ–≤–∏—â–∞
                    torso.CanCollide = false -- –û—Ç–∫–ª—é—á–µ–Ω–∏–µ –∫–æ–ª–ª–∏–∑–∏–∏
                    torso.Massless = true -- –û—Ç–∫–ª—é—á–µ–Ω–∏–µ –º–∞—Å—Å—ã –¥–ª—è —Ç—É–ª–æ–≤–∏—â–∞
                else
                    torso.Size = Vector3.new(2, 2, 1) -- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π —Ä–∞–∑–º–µ—Ä —Ç—É–ª–æ–≤–∏—â–∞
                    torso.Transparency = 0 -- –ü–æ–ª–Ω–æ—Å—Ç—å—é –≤–∏–¥–∏–º–æ–µ —Ç—É–ª–æ–≤–∏—â–µ
                end
            end
        end
    end)
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è/–æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –ø–æ–¥—Å–≤–µ—Ç–∫–∏ –∏–≥—Ä–æ–∫–æ–≤
local function CreateOrUpdateHighlight(player)
    if player.Character then
        local highlight = player.Character:FindFirstChild("Highlight")
        if not highlight and _G.HighlightEnabled and player.Name ~= game:GetService('Players').LocalPlayer.Name then
            highlight = Instance.new("Highlight")
            highlight.Parent = player.Character
            highlight.FillColor = _G.HighlightColor
            highlight.OutlineColor = _G.HighlightColor
            highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
        elseif highlight and not _G.HighlightEnabled then
            highlight:Destroy()
        elseif highlight then
            highlight.FillColor = _G.HighlightColor
            highlight.OutlineColor = _G.HighlightColor
        end
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –ø–æ–¥—Å–≤–µ—Ç–∫–æ–π
local function UpdateHighlights()
    game:GetService('RunService').RenderStepped:Connect(function()
        for _, player in pairs(game:GetService('Players'):GetPlayers()) do
            if player.Name ~= game:GetService('Players').LocalPlayer.Name then
                CreateOrUpdateHighlight(player)
            end
        end
    end)
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –Ω–∏–∫–Ω–µ–π–º–æ–≤
local function DisplayNames()
    game:GetService('RunService').RenderStepped:Connect(function()
        for _, player in next, game:GetService('Players'):GetPlayers() do
            if player.Name ~= game:GetService('Players').LocalPlayer.Name and player.Character and player.Character:FindFirstChild("Head") then
                local nameTag = player.Character.Head:FindFirstChild("NameTag")
                if _G.ShowNames and not nameTag then
                    local billboard = Instance.new("BillboardGui", player.Character.Head)
                    billboard.Name = "NameTag"
                    billboard.Size = UDim2.new(0, 200, 0, 50)
                    billboard.Adornee = player.Character.Head
                    billboard.AlwaysOnTop = true

                    local textLabel = Instance.new("TextLabel", billboard)
                    textLabel.Size = UDim2.new(1, 0, 1, 0)
                    textLabel.Text = player.Name
                    textLabel.TextColor3 = _G.NameTagColor
                    textLabel.BackgroundTransparency = 1
                elseif not _G.ShowNames and nameTag then
                    nameTag:Destroy()
                elseif nameTag then
                    nameTag.TextLabel.TextColor3 = _G.NameTagColor -- –ò–∑–º–µ–Ω–µ–Ω–∏–µ —Ü–≤–µ—Ç–∞ –Ω–∏–∫–Ω–µ–π–º–æ–≤
                end
            end
        end
    end)
end

-- –°–æ–∑–¥–∞–Ω–∏–µ –≤–∫–ª–∞–¥–∫–∏ "Universal" –¥–ª—è –æ–±—â–∏—Ö –Ω–∞—Å—Ç—Ä–æ–µ–∫
local TabUniversal = Window:MakeTab({
    Name = "Universal",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- –°–ª–∞–π–¥–µ—Ä –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Å–∫–æ—Ä–æ—Å—Ç–∏
TabUniversal:AddSlider({
    Name = "speed",
    Min = 16, -- —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å
    Max = 500,
    Default = defaultWalkSpeed,
    Color = Color3.fromRGB(127, 255, 212), -- –∞–∫–≤–∞–º–∞—Ä–∏–Ω–æ–≤—ã–π —Ü–≤–µ—Ç
    Increment = 1,
    ValueName = "Speed",
    Callback = function(Value)
        _G.WalkSpeed = Value
        SetWalkSpeed()
    end    
})

-- –ß–µ–∫–±–æ–∫—Å –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è/–æ—Ç–∫–ª—é—á–µ–Ω–∏—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Å–∫–æ—Ä–æ—Å—Ç–∏
TabUniversal:AddToggle({
    Name = "enable speed",
    Default = false,
    Callback = function(Value)
        _G.SpeedEnabled = Value
        SetWalkSpeed()
    end    
})

-- –°–ª–∞–π–¥–µ—Ä –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è –ø—Ä—ã–∂–∫–∞
TabUniversal:AddSlider({
    Name = "jump power",
    Min = 50, -- —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω–∞—è —Å–∏–ª–∞ –ø—Ä—ã–∂–∫–∞
    Max = 500,
    Default = defaultJumpPower,
    Color = Color3.fromRGB(127, 255, 212), -- –∞–∫–≤–∞–º–∞—Ä–∏–Ω–æ–≤—ã–π —Ü–≤–µ—Ç
    Increment = 1,
    ValueName = "Jump Power",
    Callback = function(Value)
        _G.JumpPower = Value
        SetJumpPower()
    end    
})

-- –ß–µ–∫–±–æ–∫—Å –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è/–æ—Ç–∫–ª—é—á–µ–Ω–∏—è –∏–∑–º–µ–Ω–µ–Ω–∏—è –ø—Ä—ã–∂–∫–∞
TabUniversal:AddToggle({
    Name = "enable jump",
    Default = false,
    Callback = function(Value)
        _G.JumpEnabled = Value
        SetJumpPower()
    end    
})

-- –°–æ–∑–¥–∞–Ω–∏–µ –≤–∫–ª–∞–¥–∫–∏ "Hitbox" –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–æ–≤ —Ç–µ–ª–∞
