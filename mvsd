    Name = "enable esp",
    Default = false,
    Callback = function(Value)
        _G.HighlightEnabled = Value
        UpdateHighlights()
    end    
})

-- –ß–µ–∫–±–æ–∫—Å –¥–ª—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è/–æ—Ç–∫–ª—é—á–µ–Ω–∏—è –Ω–∏–∫–Ω–µ–π–º–æ–≤ –∏–≥—Ä–æ–∫–æ–≤
TabVisual:AddToggle({
    Name = "name tags",
    Default = false, -- –ù–∏–∫–Ω–µ–π–º—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é –≤—ã–∫–ª—é—á–µ–Ω—ã
    Callback = function(Value)
